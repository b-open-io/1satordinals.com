<script type="application/javascript">
  document.addEventListener("DOMContentLoaded", () => {
  let ok, fk;
  const fkStr = localStorage.getItem('1satfk');
  const okStr = localStorage.getItem("1satok");
  const migrationCompleted = localStorage.getItem("migrationCompleted");

  if (fkStr && okStr && !migrationCompleted) {
    try {
      fk = JSON.parse(fkStr);
      ok = JSON.parse(okStr);
      showMigrationModal();
    } catch (error) {
      console.error('Error parsing stored keys:', error);
    }
  }

  let newWindow;

  function showModal(title, content, primaryButton, secondaryButton) {
    const modalHTML = `
      <div class="modal-backdrop">
        <div class="modal">
          <div class="modal-close">&times;</div>
          <h2 class="modal-title">${title}</h2>
          <div class="modal-content">${content}</div>
          <div class="modal-buttons">
            ${secondaryButton ? `<button class="modal-button modal-button-secondary">${secondaryButton.text}</button>` : ''}
            <button class="modal-button modal-button-primary">${primaryButton.text}</button>
          </div>
        </div>
      </div>
    `;

    const modalElement = document.createElement('div');
    modalElement.innerHTML = modalHTML;
    document.body.appendChild(modalElement);

    const closeModal = () => {
      document.body.removeChild(modalElement);
    };

    modalElement.querySelector('.modal-backdrop').addEventListener('click', (e) => {
      if (e.target === modalElement.querySelector('.modal-backdrop')) {
        closeModal();
      }
    });

    modalElement.querySelector('.modal-close').addEventListener('click', closeModal);

    modalElement.querySelector('.modal-button-primary').addEventListener('click', () => {
      primaryButton.action();
      closeModal();
    });

    if (secondaryButton) {
      modalElement.querySelector('.modal-button-secondary').addEventListener('click', () => {
        secondaryButton.action();
        closeModal();
      });
    }
  }

  function showMigrationModal() {
    showModal(
      "Migrate 1Sat Wallet",
      "A 1Sat Wallet was found in this browser. Click Migrate Wallet to move your keys to the new market website, 1Sat.Market.",
      { text: "Migrate Wallet", action: initiateKeyMigration },
      { text: "Download Keys", action: downloadKeys }
    );
  }

  function showMigrationCompleteModal() {
    showModal(
      "Migration Complete",
      "Your keys have been successfully migrated to 1Sat.Market.",
      { text: "OK", action: () => {} }
    );
  }

  function showTryAgainModal() {
    showModal(
      "Already Logged In",
      "You are already logged in to 1Sat.Market with a different key. Please sign out first and try again.",
      { text: "Try Again", action: initiateKeyMigration }
    );
  }

  function downloadKeys() {
    const keys = JSON.stringify({ ordPk: ok, payPk: fk });
    const blob = new Blob([keys], { type: 'application/json' });
    const url = URL.createObjectURL(blob);
    const a = document.createElement('a');
    a.href = url;
    a.download = '1satordinals.com_backup.json';
    document.body.appendChild(a);
    a.click();
    document.body.removeChild(a);
    URL.revokeObjectURL(url);
    localStorage.setItem("migrationCompleted", true)
  }

  function initiateKeyMigration() {
    newWindow = window.open('https://1sat.market/wallet/import', 'MigrateWindow', 'width=800,height=600');
    
    if (newWindow) {
      window.addEventListener('message', handleMigrationResponse);

      const checkWindowReadyInterval = setInterval(() => {
        if (newWindow.closed) {
          clearInterval(checkWindowReadyInterval);
          return;
        }

        try {
          newWindow.postMessage({ type: 'CHECK_READY' }, 'https://1sat.market');
        } catch (e) {
          // The window might not be ready yet, we'll try again
        }
      }, 100);

      window.addEventListener('message', function readyListener(event) {
        if (event.origin !== 'https://1sat.market') return;
        if (event.data.type === 'READY') {
          clearInterval(checkWindowReadyInterval);
          newWindow.postMessage({ type: 'MIGRATE_KEYS', payload: { ordPk: ok, payPk: fk } }, 'https://1sat.market');
          window.removeEventListener('message', readyListener);
        }
      });
    } else {
      alert('Please allow popups for this site to migrate your keys.');
    }
  }

  function handleMigrationResponse(event) {
  console.log("Received message:", event.data);
  if (event.origin !== 'https://1sat.market') return;

  if (event.data.type === 'MIGRATION_SUCCESS') {
    console.log("Migration successful");
    localStorage.setItem("migrationCompleted", "true");
    showMigrationCompleteModal();
    if (newWindow) {
      newWindow.close();
      window.removeEventListener('message', handleMigrationResponse);
    }
  } else if (event.data.type === 'MIGRATION_FAILURE') {
    console.log("Migration failed");
    alert('Key migration failed. Please try again.');
  } else if (event.data.type === 'ALREADY_LOGGED_IN') {
    console.log("Already logged in");
    showTryAgainModal();
    if (newWindow) {
      newWindow.close();
      window.removeEventListener('message', handleMigrationResponse);
    }
  }
}
});
